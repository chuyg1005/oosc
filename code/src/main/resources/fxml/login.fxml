<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="150.0" prefWidth="400.0" stylesheets="@../css/login.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" prefHeight="63.0" prefWidth="390.0">
         <children>
            <Label text="用户名：" />
            <TextField fx:id="username" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="30.0" prefWidth="146.0" />
            <ComboBox fx:id="comboBox" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onAction="#changeState" prefHeight="30.0" prefWidth="133.0" visibleRowCount="2">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </ComboBox>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="60.0" prefWidth="389.0">
         <children>
            <Label text="密码：">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <padding>
                  <Insets left="7.0" right="7.0" />
               </padding>
            </Label>
            <PasswordField fx:id="password" prefHeight="30.0" prefWidth="300.0">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </PasswordField>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="54.0" prefWidth="391.0">
         <children>
            <Button mnemonicParsing="false" onAction="#register" text="注册">
               <HBox.margin>
                  <Insets right="40.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#login" text="登录">
               <HBox.margin>
                  <Insets left="40.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
